<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ksrct.ksrctapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ksrct_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ksrct_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LaunchAct"
            android:theme="@android:style/Theme.NoDisplay">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginAct" />
        <activity android:name=".About" />
        <activity android:name=".feedback" />
        <activity android:name=".student.MainActivity" />
        <activity android:name=".staff.StaffMainActivity"></activity>
    </application>

</manifest>